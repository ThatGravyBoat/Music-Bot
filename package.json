{
  "name": "music_bot",
  "version": "1.0.0",
  "description": "Music Bot for discord that uses buttons and slash commands only.",
  "exports": "./dist/index.js",
  "engines": {
    "node": ">=16.6.0"
  },
  "repository": "https://github.com/ThatGravyBoat/Music-Bot.git",
  "author": "ThatGravyBoat",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "yarn tsc",
    "start": "yarn build && node --experimental-json-modules --no-warnings ./dist/index.js",
    "generate": "yarn build && node --experimental-json-modules --no-warnings ./dist/generator/generate.js"
  },
  "dependencies": {
    "@discordjs/opus": "^0.7.0",
    "@discordjs/rest": "^0.3.0",
    "discord-player": "^5.2.0",
    "discord.js": "^13.6.0",
    "ffmpeg-static": "^4.4.1",
    "source-map-support": "^0.5.21",
    "typescript": "^4.5.5"
  },
  "devDependencies": {
    "@types/node": "^17.0.12",
    "@types/source-map-support": "^0.5.4",
    "discord-api-types": "^0.26.1"
  }
}